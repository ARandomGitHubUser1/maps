{
  "compilerOptions": {
    "target": "es5",
    "module": "AMD",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"]
}